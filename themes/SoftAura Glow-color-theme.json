{
	"name": "SoftAura Glow",
	"colors": {
		"editor.background": "#182028",
		"editor.foreground": "#e0e0e0",
		"activityBarBadge.background": "#12e51d",
		"sideBarTitle.foreground": "#70f72d",
		"sideBar.background": "#1820289d",
		"sideBar.border": "#6446a99d",
		"sideBarSectionHeader.background": "#3833489d",
		"editorWidget.foreground": "#bb00ff",
		"statusBarItem.remoteBackground": "#ea00ffec",
		"statusBarItem.hoverForeground": "#40ff00",
		"terminalCursor.background": "#21dd00",
		"icon.foreground": "#2bea09",
		"terminal.foreground": "#38e116",
		"terminal.hoverHighlightBackground": "#11ff00",
		"statusBar.background": "#383f46",
		
		
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF8C00"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#66c2ff"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#b3ff66"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FF4F81"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
